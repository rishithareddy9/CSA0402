#include <stdio.h>
#include <string.h>

int main() {
    char user[2][20] = {"User1", "User2"};
    char files[2][3][20] = {
        {"file1", "file2", "file3"},
        {"fileA", "fileB", "fileC"}
    };

    int i, j;

    for (i = 0; i < 2; i++) {
        printf("\n%s Directory:\n", user[i]);
        for (j = 0; j < 3; j++) {
            printf("  %s\n", files[i][j]);
        }
    }

    return 0;
}
