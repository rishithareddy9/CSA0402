#include <stdio.h>

int main() {
    int block[5] = {100, 500, 200, 300, 600};
    int process[4] = {212, 417, 112, 426};
    int i, j, alloc[4];

    for (i = 0; i < 4; i++) alloc[i] = -1;

    for (i = 0; i < 4; i++) {
        int worst = -1;
        for (j = 0; j < 5; j++) {
            if (block[j] >= process[i]) {
                if (worst == -1 || block[j] > block[worst])
                    worst = j;
            }
        }
        if (worst != -1) {
            alloc[i] = worst;
            block[worst] -= process[i];
        }
    }

    printf("Process\tSize\tBlock\n");
    for (i = 0; i < 4; i++) {
        printf("%d\t%d\t", i, process[i]);
        if (alloc[i] != -1) printf("%d\n", alloc[i]);
        else printf("Not Allocated\n");
    }
    return 0;
}
