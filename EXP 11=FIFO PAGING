#include <stdio.h>

int main() {
    int pages[20], frames[3], n = 12;
    int i, j, k = 0, faults = 0;

    int ref[] = {1,2,3,4,1,2,5,1,2,3,4,5};

    for (i = 0; i < 3; i++)
        frames[i] = -1;

    for (i = 0; i < n; i++) {
        int found = 0;
        for (j = 0; j < 3; j++) {
            if (frames[j] == ref[i]) {
                found = 1;
                break;
            }
        }

        if (!found) {
            frames[k] = ref[i];
            k = (k + 1) % 3;
            faults++;
        }
    }

    printf("Total Page Faults = %d\n", faults);
    return 0;
}
OUTPUT 
Total Page Faults = 9
